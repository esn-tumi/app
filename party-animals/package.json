{
  "name": "@tumi/party-animals",
  "private": true,
  "description": "Remix app for party animals registration and management",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn prisma generate && yarn build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"yarn dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@prisma/client": "^3.15.1",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@tailwindcss/forms": "^0.5.2",
    "cross-env": "^7.0.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "remix-auth": "^3.2.2",
    "remix-auth-auth0": "1.3.7",
    "stripe": "^9.8.0",
    "v8n": "^1.4.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "postcss": "^8.4.14",
    "prisma": "^3.15.1",
    "tailwindcss": "^3.1.2",
    "typescript": "~4.7.3"
  },
  "engines": {
    "node": ">=14"
  }
}
