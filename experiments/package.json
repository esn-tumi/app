{
  "name": "@tumi/experiments",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "concurrently --kill-others-on-fail \"yarn start\" \"yarn codegen --watch\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "update": "ng update -C @angular/cli @angular/core",
    "codegen": "yarn graphql-codegen",
    "codegen:watch": "yarn graphql-codegen --watch",
    "lint": "ng lint"
  },
  "private": true,
