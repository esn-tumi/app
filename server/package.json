{
  "name": "@tumi/server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .env.development -- ts-node-dev --no-notify --respawn src/server",
    "test": "yarn prisma:reset-test && dotenv -e .env.test -- ts-node-dev --no-notify --respawn --transpile-only src/server",
    "dev:full-db": "ts-node-dev --no-notify --respawn src/server",
    "start": "node dist/server",
    "clean": "rimraf dist",
    "copy": "copyfiles -u 3 src/generated/prisma/** dist/generated/prisma && copyfiles -u 2 src/assets/** dist/assets",
    "build": "yarn clean && yarn generate && tsc && yarn copy",
    "build:production": "yarn clean && yarn generate && tsc -p tsconfig.production.json && yarn copy",
    "generate": "yarn generate:prisma",
    "generate:prisma": "prisma generate",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prisma:seed-dev": "dotenv -e .env.development -- ts-node prisma/seed.ts",
    "prisma:studio-dev": "dotenv -e .env.development -- prisma studio",
    "prisma:reset-dev": "dotenv -e .env.development -- yarn prisma db push --accept-data-loss",
    "prisma:reset-test": "dotenv -e .env.test -- yarn prisma migrate reset --skip-seed --force && dotenv -e .env.test -- yarn prisma db push --accept-data-loss && dotenv -e .env.test -- yarn prisma db seed",
    "prisma:seed-test": "dotenv -e .env.test -- yarn prisma db seed",
    "test-old": "yarn docker:up && dotenv -e .env.test -- yarn prisma migrate reset --force && dotenv -e .env.test -- jest -i --detectOpenHandles",
    "prepare-test": "yarn docker:up && dotenv -e .env.test -- yarn prisma migrate reset --force"
  },
  "dependencies": {
    "@angular/google-maps": "^14.2.2",
    "@azure/storage-blob": "^12.11.0",
    "@envelop/auth0": "^3.6.0",
    "@envelop/core": "^2.6.0",
    "@envelop/graphql-jit": "^4.6.0",
    "@envelop/response-cache": "^3.2.0",
    "@envelop/sentry": "^3.8.1",
    "@graphql-hive/client": "^0.18.5",
    "@graphql-yoga/node": "^2.13.13",
    "@pathcheck/dcc-sdk": "^0.0.27",
    "@pothos/core": "^3.21.0",
    "@pothos/plugin-prisma": "^3.34.0",
    "@pothos/plugin-scope-auth": "3.13.0",
    "@pothos/plugin-simple-objects": "^3.5.0",
    "@pothos/plugin-tracing": "^0.4.0",
    "@pothos/tracing-sentry": "^0.6.0",
    "@prisma/client": "^4.3.1",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/integrations": "^7.13.0",
    "@sentry/node": "^7.13.0",
    "@sentry/tracing": "^7.13.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "es6-promise-pool": "^2.5.0",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.18.0",
    "ical-generator": "^3.5.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.4",
    "lodash": "^4.17.21",
    "luxon": "^3.0.4",
    "marked": "^4.1.0",
    "mjml": "^4.13.0",
    "node-cron": "^3.0.2",
    "node-fetch-native": "^0.1.7",
    "pdf2pic": "^2.1.4",
    "prom-client": "^14.1.0",
    "qrcode": "^1.5.1",
