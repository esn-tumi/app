{
  "name": "@tumi/server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .env.development -- ts-node-dev --no-notify --respawn --transpile-only src/server",
    "dev:full-db": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "start": "node dist/server",
    "clean": "rimraf dist",
    "copy": "copyfiles -u 3 src/generated/prisma/** dist/generated/prisma && copyfiles -u 3 src/generated/nexus-prisma/** dist/generated/nexus-prisma",
    "build": "yarn clean && yarn generate && tsc && yarn copy",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "yarn docker:up && dotenv -e .env.test -- yarn prisma migrate reset --force && dotenv -e .env.test -- jest -i --detectOpenHandles",
    "prepare-test": "yarn docker:up && dotenv -e .env.test -- yarn prisma migrate reset --force"
  },
  "dependencies": {
    "@envelop/auth0": "^3.2.0",
    "@envelop/core": "^2.2.0",
    "@envelop/graphql-middleware": "^3.2.0",
    "@envelop/parser-cache": "^4.2.0",
    "@envelop/response-cache": "^2.2.0",
    "@envelop/sentry": "^3.2.0",
    "@envelop/validation-cache": "^4.2.0",
    "@graphql-hive/client": "^0.14.2",
    "@pathcheck/dcc-sdk": "^0.0.27",
    "@prisma/client": "^3.11.1",
    "@sendgrid/mail": "^7.6.2",
    "@sentry/node": "^6.19.3",
    "@sentry/tracing": "^6.19.3",
    "canvas": "^2.9.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-jwt": "^6.1.1",
    "got": "^12.0.3",
    "graphql": "^16.3.0",
    "graphql-helix": "^1.12.0",
    "graphql-middleware": "^6.1.21",
    "graphql-shield": "^7.5.0",
    "ical-generator": "^3.4.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "nexus": "^1.3.0",
    "nexus-prisma": "0.36.0-next.2",
    "node-fetch": "^2.6.7",
    "qrcode": "^1.5.0",
    "stripe": "^8.214.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
    "@types/compression": "^1.7.2",
    "@types/copyfiles": "^2.4.1",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.4",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.181",
    "@types/luxon": "^2.3.1",
    "@types/node": "^17.0.23",
    "@types/prettier": "^2.4.4",
    "@types/qrcode": "^1.4.2",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/parser": "^5.17.0",
    "apollo-server-plugin-base": "^3.5.2",
    "copyfiles": "^2.4.1",
    "dayjs": "^1.11.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.34",
    "prettier": "^2.6.1",
    "prisma": "^3.11.1",
    "rimraf": "^3.0.2",
    "rrule": "^2.6.9",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.6.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
