    "stripe": "^10.11.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@apollo/utils.logger": "^1.0.0",
    "@faker-js/faker": "^7.5.0",
    "@types/compression": "^1.7.2",
    "@types/copyfiles": "^2.4.1",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.6",
    "@types/express": "^4.17.14",
    "@types/express-jwt": "^6.0.4",
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.185",
    "@types/luxon": "^3.0.1",
    "@types/node": "^18.7.22",
