    "@sentry/node": "^7.47.0",
    "@sentry/tracing": "^7.47.0",
    "@whatwg-node/fetch": "0.8.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "es6-promise-pool": "^2.5.0",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.21.3",
    "graphql-yoga": "^3.9.1",
    "ical-generator": "^4.0.0",
